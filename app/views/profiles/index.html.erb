<h1>Listing profiles</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Name</th>
      <th>Imagetype</th>
      <th>Image 2</th>
      <th>Prof</th>
      <th>Image 3</th>
      <th>Desk</th>
      <th>Image 4</th>
      <th>Image 5</th>
      <th>Price</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.image %></td>
        <td><%= profile.name %></td>
        <td><%= profile.imagetype %></td>
        <td><%= profile.image_2 %></td>
        <td><%= profile.prof %></td>
        <td><%= profile.image_3 %></td>
        <td><%= profile.desk %></td>
        <td><%= profile.image_4 %></td>
        <td><%= profile.image_5 %></td>
        <td><%= profile.price %></td>
        <td><%= profile.category_id %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
